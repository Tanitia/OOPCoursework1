<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.AdminLoginController">
   <children>
      <Label layoutX="250.0" layoutY="50.0" text="Admin Log-in">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="161.0" layoutY="126.0" text="Name" />
      <Label layoutX="152.0" layoutY="168.0" text="Password" />
      <TextField id="ALINameBox" fx:id="ALINameBox" layoutX="236.0" layoutY="122.0" promptText="Enter Name" />
      <Button id="ALIButton" fx:id="APLoginButton" layoutX="275.0" layoutY="285.0" mnemonicParsing="false" onAction="#adminLoginOK" text="Log-in" />
      <PasswordField fx:id="ALIPasswordBox" layoutX="236.0" layoutY="164.0" />
      <Label fx:id="ALIErrorLabel" layoutX="286.0" layoutY="240.0" textFill="#c70404">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Button fx:id="ALCancel" layoutX="275.0" layoutY="327.0" mnemonicParsing="false" onAction="#ALGoBack" text="Cancel" />
   </children>
</AnchorPane>
