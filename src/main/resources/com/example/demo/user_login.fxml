<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.UserLoginController">
   <children>
      <Label layoutX="249.0" layoutY="46.0" text="User Log-in">
         <font>
            <Font size="20.0" />
         </font></Label>
      <Label layoutX="161.0" layoutY="126.0" text="ID" />
      <Label layoutX="152.0" layoutY="168.0" text="Password" />
      <TextField id="ULIIDBox" fx:id="ULIIDBox" layoutX="236.0" layoutY="122.0" promptText="Enter Name" />
      <Button id="ULIButton" fx:id="ULLogin" layoutX="275.0" layoutY="285.0" mnemonicParsing="false" onAction="#logInAction" text="Log-in" />
      <PasswordField fx:id="ULIPasswordBox" layoutX="236.0" layoutY="164.0" />
      <Label fx:id="ULIErrorLabel" layoutX="286.0" layoutY="254.0" textFill="#d20101">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Button fx:id="ULIBackButton" layoutX="275.0" layoutY="323.0" mnemonicParsing="false" onAction="#ULIBackButton" text="Back" />
   </children>
</AnchorPane>
