<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.CreateCandidateController">
   <children>
      <Label layoutX="244.0" layoutY="51.0" text="Add Candidate">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="133.0" layoutY="134.0" text="Name" />
      <Label layoutX="143.0" layoutY="158.0" text="ID" />
      <Label layoutX="120.0" layoutY="182.0" text="Description" />
      <TextField id="USUName" fx:id="ACNameBox" layoutX="226.0" layoutY="130.0" promptText="Name" />
      <TextField id="USUAddress" fx:id="ACIDBox" layoutX="226.0" layoutY="154.0" promptText="ID" />
      <TextField id="USUIDNum" fx:id="ACDescriptionBox" layoutX="226.0" layoutY="178.0" prefHeight="104.0" prefWidth="149.0" promptText="Description" />
      <Button fx:id="USUOkay" layoutX="278.0" layoutY="307.0" mnemonicParsing="false" onAction="#confirmCandidateCreate" text="Okay" />
      <Label fx:id="ACErrorLabel" layoutX="286.0" layoutY="348.0" textFill="#ae0000">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Button fx:id="ACBackButton" layoutX="280.0" layoutY="344.0" mnemonicParsing="false" onAction="#ACBackGo" text="Back" />
   </children>
</AnchorPane>
